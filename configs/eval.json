{
  "device": "cuda",
  "data": {
    "batch_size": 1,
    "num_workers": 4,
    "image_size": 101,
    "channels": 1,
    "h5_path": "/data/eval.h5",
    "h5_lr_key": "lr",
    "h5_hr_key": "hr",
    "augment": false
  },
  "model": {
    "module": "network",
    "backbone": "unet_res",
    "diffusion_steps": 1000,
    "checkpoint_path": "./SR/autodl_weight/model1020focus-1.pth"
  },
  "vae": {
    "enabled": false,
    "checkpoint_path": "./SR/autodl_weight/vae_best-1.pt",
    "encoder_key": "enc",
    "decoder_key": "dec"
  },
  "sampler": {
    "threshold": 0.05,
    "type": "ddim",
    "ddim_steps": 50,
    "eta": 1.0,
    "simple_var": false
  },
  "output": {
    "root": "SR/eval_results_1020focus-1",
    "save_images": true
  }
}