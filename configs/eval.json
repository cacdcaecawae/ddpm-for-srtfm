{
  "device": "cuda",
  "data": {
    "lr_root": "./data/eval/TFM",
    "hr_root": "./data/eval/hr",
    "batch_size": 1,
    "num_workers": 4,
    "image_size": 400,
    "channels": 1
  },
  "model": {
    "module": "network",
    "backbone": "unet_res",
    "diffusion_steps": 1000,  
    "checkpoint_path": "./SR/autodl_weight/modelSR1009_07.pth"
  },
  "vae": {
    "enabled": false,
    "checkpoint_path": "./SR/autodl_weight/vae_best-1.pt",
    "encoder_key": "enc",
    "decoder_key": "dec"
  },
  "sampler": {
    "threshold": 0.98,
    "type": "ddim",
    "ddim_steps": 20,
    "eta": 1.0,
    "simple_var": false
  },
  "output": {
    "root": "SR/eval_results_1009_07",
    "save_images": true
  }
}
